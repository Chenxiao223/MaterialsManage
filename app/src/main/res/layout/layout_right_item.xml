<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffff"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/right_item_textview0"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="物资编码"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/right_item_textview1"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="规格型号"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/right_item_textview2"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="计量单位"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/right_item_textview3"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="分类名称"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/right_item_textview4"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="车辆信息"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/right_item_textview5"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="默认仓库"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/right_item_textview6"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="默认货位"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/right_item_textview7"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="旧编码"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/right_item_textview8"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:gravity="center"
        android:text="RFID"
        android:textSize="12dp" />

</LinearLayout>